
getgenv().a = false;
getgenv().b = false;

local pochita = game:GetService("Workspace").pochita
local plr = game:GetService("Players").LocalPlayer
local RunService = game:GetService("RunService")
local pochitamood = pochita.MoodStat
local workspace = game:GetService("Workspace")

local plrhead = plr.Character.Head
function AutoClick()
	while getgenv().a and RunService.RenderStepped:Wait() do
		pochita.Part.RemoteEvent:FireServer(plr)
	end
end
function AutoCollect()
	while wait() do
		if not getgenv().b then break end
		for _,v in pairs(workspace:GetDescendants()) do
			if v:IsA("TouchInterest") then
			firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 0) --0 is touch
			wait(0.1)
			firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 1) -- 1 is untouch
			end
		end
	end
end


local lib = loadstring(game:HttpGet"https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/Vape.txt")()

local win = lib:Window("CatWare",Color3.fromRGB(44, 120, 224), Enum.KeyCode.RightControl)

local tab = win:Tab("Main")

tab:Button("Pochita Mood max", function()
	pochitamood.Value = 100
end)
tab:Toggle("AutoClick",false, function(t)
	getgenv().a = t
	if t then
		AutoClick()
	end
end)
tab:Toggle("AutoCollect",false, function(b)
	getgenv().a = b
	if b then
		AutoCollect()
	end
end)

tab:Slider("WalkSpeed",0,100,30, function(t)
		plr.Character.Humanoid.WalkSpeed = t
end)

tab:Dropdown("Dropdown",{"Option 1","Option 2","Option 3","Option 4","Option 5"}, function(t)
print(t)
end)

tab:Colorpicker("Colorpicker",Color3.fromRGB(255,0,0), function(t)
print(t)
end)

tab:Textbox("Textbox",true, function(t)
print(t)
end)

tab:Bind("Bind",Enum.KeyCode.RightShift, function()
print("Pressed!")
end)

tab:Label("Label")

local changeclr = win:Tab("Change UI Color")

changeclr:Colorpicker("Change UI Color",Color3.fromRGB(44, 120, 224), function(t)
lib:ChangePresetColor(Color3.fromRGB(t.R * 255, t.G * 255, t.B * 255))
end)
